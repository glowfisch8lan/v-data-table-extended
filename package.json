{
  "name": "v-data-table-extended",
  "version": "0.0.7",
  "description": "Extended Vuetify v-data-table component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/glowfisch8lan/v-data-table-extended.git"
  },
  "main": "dist/v-data-table-extended.umd.js",
  "module": "dist/v-data-table-extended.esm.js",
  "unpkg": "dist/v-data-table-extended.min.js",
  "scripts": {
    "server": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/v-data-table-extended.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/v-data-table-extended.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/v-data-table-extended.min.js"
  },
  "files": [
    "dist/*"
  ],
  "homepage": "https://github.com/glowfisch8lan/v-data-table-extended",
  "bugs": {
    "url": "https://github.com/glowfisch8lan/v-data-table-extended/issues"
  },
  "author": "D. Grigorov",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.21",
    "portal-vue": "^2.1.7",
    "rollup-plugin-babel": "^4.4.0",
    "vue": "^2.6.14",
    "vue-cli": "^2.9.6",
    "vuetify": "^2.6.7",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli": "^5.0.6",
    "@vue/cli-service": "^5.0.6",
    "deepmerge": "^4.2.2",
    "node-sass": "^7.0.1",
    "rollup": "^1.17.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-vue": "^5.0.1",
    "sass": "~1.32",
    "sass-loader": "^13.0.2",
    "vue-template-compiler": "^2.6.10"
  }
}
